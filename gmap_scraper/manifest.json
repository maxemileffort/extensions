{
  "manifest_version": 3,
  "name": "Google Maps Scraper",
  "version": "1.0",
  "description": "Scrapes data from Google Maps 'feed' div.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://www.google.com/maps/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },

  "background": {
    "service_worker": "background.js"
  }
}
