{
    "manifest_version": 3,
    "name": "Ad Remover",
    "version": "1.0",
    "description": "Removes ads from web pages",
    "permissions": ["activeTab", "tabs"],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["removeAds.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
  